};\r\n    default:\r\n      return state;\r\n  }\r\n}\r\n\r\n/* 스토어 만들기 */\r\nconst store = createStore(reducer);\r\nconsole.log(store.getState());\r\n\r\n// 스토어안에 들어있는 상태가 바뀔 때마다 호출되는 listender 함수\r\nconst listener = () => {\r\n  const state = store.getState();\r\n  console.log(state);\r\n};\r\n\r\nconst unsubscribe = store.subscribe(listener);\r\n\r\nstore.dispatch(increase());\r\nstore.dispatch(decrease());\r\nstore.dispatch(changeText(\"안녕하세요\"));\r\nstore.dispatch(addToList({ id: 1, text: \"와우\" }));\r\n","C:\\Users\\USER\\learn-redux\\src\\index.js",[],["112","113"],"C:\\Users\\USER\\learn-redux\\src\\modules\\todos.js",[],"C:\\Users\\USER\\learn-redux\\src\\modules\\counter.js",[],"C:\\Users\\USER\\learn-redux\\src\\modules\\index.js",["114"],"C:\\Users\\USER\\learn-redux\\src\\components\\counter.js",[],"C:\\Users\\USER\\learn-redux\\src\\components\\todos.js",[],"C:\\Users\\USER\\learn-redux\\src\\containers\\todosContainer.js",[],"C:\\Users\\USER\\learn-redux\\src\\containers\\counterContainer.js",[],"C:\\Users\\USER\\learn-redux\\src\\containers\\counterContainer2.js",[],"C:\\Users\\USER\\learn-redux\\src\\modules\\counter2.js",["115"],"import React from \"react\";\r\n\r\nconst INCREASE = \"counter2/INCREASE\";\r\nconst DECREASE = \"counter2/DECREASE\";\r\n\r\nexport const increase = () => ({ type: INCREASE });\r\nexport const decrease = () => ({ type: DECREASE });\r\nconst initialState = { number: 0 };\r\n\r\nexport default function Counter2(state = initialState, action) {\r\n  switch (action.type) {\r\n    case INCREASE:\r\n      return {\r\n        ...state,\r\n        number: state.number + 1,\r\n      };\r\n    case DECREASE:\r\n      return {\r\n        ...state,\r\n        number: state.number - 1,\r\n      };\r\n    default:\r\n      return state;\r\n  }\r\n}\r\n","C:\\Users\\USER\\learn-redux\\src\\components\\counter2.jsx",[],["116","117"],"C:\\Users\\USER\\learn-redux\\src\\containers\\counterContainer3.js",[],"C:\\Users\\USER\\learn-redux\\src